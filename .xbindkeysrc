"pamixer -t"
  XF86AudioMute

"pamixer -d 10"
  XF86AudioLowerVolume

"pamixer -i 10"
  XF86AudioRaiseVolume

"pamixer -d 1"
  Shift + XF86AudioLowerVolume

"pamixer -i 1"
  Shift + XF86AudioRaiseVolume

"gmux_backlight -100"
  XF86MonBrightnessDown

"gmux_backlight +100"
  XF86MonBrightnessUp

"gmux_backlight -10"
  Shift + XF86MonBrightnessDown

"gmux_backlight +10"
  Shift + XF86MonBrightnessUp

"xleds -dec 10"
  XF86KbdBrightnessDown

"xleds -inc 10"
  XF86KbdBrightnessUp

"xleds -dec 1"
  Shift + XF86KbdBrightnessDown

"xleds -inc 1"
  Shift + XF86KbdBrightnessUp

"gnome-screenshot -i"
  XF86LaunchB

"lock"
  Alt+Mod4 + l

"thunar"
  Mod4 + f
