"pamixer -t"
  XF86AudioMute

"pamixer -d 10"
  XF86AudioLowerVolume

"pamixer -i 10"
  XF86AudioRaiseVolume

"pamixer -d 1"
  Shift + XF86AudioLowerVolume

"pamixer -i 1"
  Shift + XF86AudioRaiseVolume

"xleds screen - 10%"
  XF86MonBrightnessDown

"xleds screen + 10%"
  XF86MonBrightnessUp

"xleds screen - 1%"
  Shift + XF86MonBrightnessDown

"xleds screen + 1%"
  Shift + XF86MonBrightnessUp

"xleds key - 10"
  XF86KbdBrightnessDown

"xleds key + 10"
  XF86KbdBrightnessUp

"xleds key - 1"
  Shift + XF86KbdBrightnessDown

"xleds key + 1"
  Shift + XF86KbdBrightnessUp

"gnome-screenshot -i"
  XF86LaunchB

"lock"
  Alt+Mod4 + l

"thunar"
  Mod4 + f
