"set-volume -t"
  XF86AudioMute

"set-volume -d 10"
  XF86AudioLowerVolume

"set-volume -i 10"
  XF86AudioRaiseVolume

"set-volume -d 1"
  Shift + XF86AudioLowerVolume

"set-volume -i 1"
  Shift + XF86AudioRaiseVolume

"gmux_backlight -100"
  XF86MonBrightnessDown

"gmux_backlight +100"
  XF86MonBrightnessUp

"gmux_backlight -10"
  Shift + XF86MonBrightnessDown

"gmux_backlight +10"
  Shift + XF86MonBrightnessUp

"xleds -dec 10"
  XF86KbdBrightnessDown

"xleds -inc 10"
  XF86KbdBrightnessUp

"xleds -dec 1"
  Shift + XF86KbdBrightnessDown

"xleds -inc 1"
  Shift + XF86KbdBrightnessUp

"gnome-screenshot -i"
  XF86LaunchB

"gnome-screensaver-command -l"
  Shift+Mod4 + l
